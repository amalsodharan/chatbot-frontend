{
  "name": "chatbot",
  "version": "0.0.0",
  "scripts": {
    "build:ssr": "npm run build:client-and-server-bundles",
    "build:client-and-server-bundles": "node_modules/.bin/ng build --configuration production && node_modules/.bin/ng run chatbot:server",
    "serve:ssr": "node dist/chatbot/server/main.js",
    "watch": "node_modules/.bin/ng build --watch --configuration development",
    "test": "node_modules/.bin/ng test"
  },
  "private": true,
